{
    "data": {
        "data_path": "./data",
        "dataset_option": ["cifar10", "fashionmnist"],
        "dataset": "cifar10",
        "batch_size": 128,
        "num_channels": 3,
        "train_test_split_perc": 0.8,
        "image_size": 32
        },
    "sampler": {
        "sampler_options": ["sasgld", "sacsgld", "sgld", "csgld"],
        "sampler": "sacsgld",
        "sparse": true,
        "update_rate": 0.001,
        "rho_0": 10000,
        "rho_1": 1,
        "u": 50
    },
    "model": {
        "model_options": ["LeNet5", "AlexNet", "VGG6", "ResNet18", "GoogLeNet"],
        "model_name": "ResNet18"
    }, 
    "training":{
        "epoches": 300,
        "numIter": 100000,
        "cycles": 6,
        "gamma": 5e-6,
        "alpha_0": 0.5,
        "threshold": 0.25
    }
}